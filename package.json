{
  "name": "codedeck",
  "version": "1.0.0",
  "description": "Visual wrapper for Claude Code SDK",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && tsc -p tsconfig.electron.json && electron-builder",
    "download-cli": "node scripts/download-railway-cli.cjs",
    "download-node": "node scripts/download-node.cjs",
    "download-git": "node scripts/download-git.cjs",
    "prebuild": "npm run download-cli && npm run download-node && npm run download-git"
  },
  "keywords": [
    "electron",
    "claude",
    "ai",
    "low-code"
  ],
  "author": "CodeDeck",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.33",
    "@excalidraw/excalidraw": "^0.18.0",
    "@google-analytics/data": "^5.2.1",
    "@heroicons/react": "^2.2.0",
    "@huggingface/transformers": "^3.8.1",
    "@iconify/react": "^6.0.2",
    "@supabase/supabase-js": "^2.78.0",
    "@xterm/addon-fit": "^0.10.0",
    "adm-zip": "^0.5.16",
    "ansi-to-html": "^0.7.2",
    "better-sqlite3": "^12.5.0",
    "canvas": "^3.2.0",
    "detect-port": "^2.1.0",
    "dotenv": "^17.2.3",
    "ffmpeg-static": "^5.3.0",
    "framer-motion": "^12.23.24",
    "image-size": "^2.0.2",
    "konva": "^10.0.12",
    "mongodb": "^6.20.0",
    "netlify-cli": "^23.12.2",
    "node-pty": "^1.1.0-beta40",
    "puppeteer": "^24.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.14",
    "react-markdown": "^10.1.0",
    "recharts": "^3.4.1",
    "sharp": "^0.34.5",
    "simple-git": "^3.29.0",
    "strip-ansi": "^7.1.2",
    "wavesurfer.js": "^7.12.1",
    "xterm": "^5.3.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/adm-zip": "^0.5.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.19.24",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "electron": "^38.7.2",
    "electron-builder": "23.0.2",
    "lucide-react": "^0.312.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vercel": "^49.1.1",
    "vite": "^5.0.12",
    "vite-plugin-electron": "^0.28.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.codedeck.app",
    "productName": "CodeDeck",
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "asarUnpack": [
      "node_modules/netlify-cli/**/*"
    ],
    "npmRebuild": false,
    "extraResources": [
      {
        "from": "resources/binaries/${platform}-${arch}",
        "to": "binaries",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "extraResources": [
        {
          "from": "resources/binaries/darwin-${arch}",
          "to": "binaries"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "extraResources": [
        {
          "from": "resources/binaries/win32-x64",
          "to": "binaries"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "extraResources": [
        {
          "from": "resources/binaries/linux-x64",
          "to": "binaries"
        }
      ]
    }
  }
}
